{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "src/"
    ],
    "includePattern": ".+\\.js$"
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "search": true,
    "default": {
      "staticFiles": {
        "include": []
      }
    }
  },
  "docdash": {
    "meta": {
      "title": "Aliwork helper",
      "description": "宜搭前端 JS 工具代码库",
      "keyword": "Aliwork 宜搭 工具库"
    },
    "search": true,
    "sectionOrder": [
      "Modules",
      "Classes",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ],
    "menu": {
      "Github": {
        "href": "https://github.com/hackingzhang/aliwork-helper",
        "target": "_blank",
        "class": "menu-item"
      },
      "Gitee码云": {
        "href": "https://gitee.com/hexzhangshuai/aliwork-helper",
        "target": "_blank",
        "class": "menu-item"
      },
      "宜搭开放API": {
        "href": "https://docs.aliwork.com/docs/developer/api/about",
        "target": "_blank",
        "class": "menu-item"
      }
    }
  }
}